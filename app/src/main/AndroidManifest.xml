<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alexandraw.myidealmenu" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".SplashScreenActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RootActivity"
            android:label="@string/title_activity_root" >
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name=".FastFoodActivity"
            android:label="@string/title_activity_fast_food" >
        </activity>
        <activity
            android:name=".IceCreamActivity"
            android:label="@string/title_activity_ice_cream" >
        </activity>
        <activity
            android:name=".DrinksActivity"
            android:label="@string/title_activity_drinks" >
        </activity>
        <activity
            android:name=".OthersActivity"
            android:label="@string/title_activity_others" >
        </activity>
        <activity
            android:name=".Secundario"
            android:label="@string/title_activity_secundario" >
        </activity>
    </application>

</manifest>
